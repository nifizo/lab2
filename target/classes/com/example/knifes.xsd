<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="knifes">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="knife" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="type" type="xs:string" />
                            <xs:element name="handy" type="xs:unsignedInt" />
                            <xs:element name="origin" type="xs:unsignedInt" />
                            <xs:element name="visual">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="length" type="xs:unsignedInt" />
                                        <xs:element name="width " type="xs:unsignedInt" />
                                        <xs:element name="material" type="xs:string" />
                                        <xs:element name="handle_material" type="xs:string"/>
                                        <xs:element name="bloodstream" type="xs:boolean" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="value" type="xs:boolean" />
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:unsignedInt" use="required" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>